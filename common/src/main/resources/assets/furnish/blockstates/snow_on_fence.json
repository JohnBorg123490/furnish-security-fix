{
  "multipart": [
    {
      "apply": {"model": "furnish:block/winter/snowy_fence_post"}
    },
    {
      "when": {"north":  true},
      "apply": {"model": "furnish:block/winter/snowy_fence_side"}
    },
    {
      "when": {"south":  true},
      "apply": {"model": "furnish:block/winter/snowy_fence_side", "y": 180}
    },
    {
      "when": {"east":  true},
      "apply": {"model": "furnish:block/winter/snowy_fence_side", "y": 90}
    },
    {
      "when": {"west":  true},
      "apply": {"model": "furnish:block/winter/snowy_fence_side", "y": 270}
    },
    {
      "when": {"ground": true},
      "apply": {"model": "furnish:block/winter/snowy_fence_ground"}
    }
  ]
}