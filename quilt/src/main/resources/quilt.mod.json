{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "furnish",
    "version": "${version}",
    "name": "Furnish",
    "description": "Adds lovely and useful furniture to Minecraft!",
    "authors": ["Me!"],
    "contact": {
      "sources": "https://github.com/Wouink/furnish"
    },
    "license": "Insert License Here",
    "icon": "assets/furnish/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.examplemod.quilt.ExampleModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  }
}
