{
  "schema_version": 1,
  "quilt_loader": {
    "group": "io.github.wouink",
    "id": "furnish",
    "version": "${version}",
    "metadata": {
      "name": "Furnish",
      "description": "Adds lovely and useful furniture to Minecraft!",
      "contributors": {
        "Wouink": "Owner"
      },
      "contact": {
        "homepage": "https://curseforge.com/minecraft/mc-mods/furnish",
        "issues": "https://github.com/Wouink/furnish/issues",
        "sources": "https://github.com/Wouink/furnish"
      },
      "icon": "assets/furnish/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "io.github.wouink.furnish.quilt.FurnishQuilt"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  }
}
